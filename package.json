{
  "name": "lexical-remark",
  "description": "This package contains Markdown helpers and functionality for Lexical using remark-parse.",
  "keywords": [
    "lexical",
    "editor",
    "rich-text",
    "markdown",
    "remark",
    "remark-parse"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "npm run clean && tsc",
    "test": "NODE_ENV=development vitest run",
    "test:preview": "vite preview --outDir html --open",
    "prepublishOnly": "npm run build",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src"
  },
  "license": "MIT",
  "version": "0.3.1",
  "type": "module",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    }
  },
  "peerDependencies": {
    "@lexical/code": "0.x",
    "@lexical/link": "0.x",
    "@lexical/list": "0.x",
    "@lexical/react": "0.x",
    "@lexical/rich-text": "0.x",
    "@lexical/utils": "0.x",
    "lexical": "0.x",
    "react": "^18.2.0"
  },
  "dependencies": {
    "@nexusmods/eslint-plugin-nexusmods": "^0.0.6",
    "eslint": "^8.41.0",
    "mdast-util-from-markdown": "^1.3.0",
    "remark-parse": "^10.0.1",
    "remark-stringify": "^10.0.2",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.2",
    "zwitch": "^2.0.4"
  },
  "devDependencies": {
    "@headlessui/react": "^1.7.15",
    "@lexical/code": "^0.11.1",
    "@lexical/headless": "^0.11.1",
    "@lexical/html": "^0.11.1",
    "@lexical/link": "^0.11.1",
    "@lexical/list": "^0.11.1",
    "@lexical/mark": "^0.11.1",
    "@lexical/react": "^0.11.1",
    "@lexical/rich-text": "^0.11.1",
    "@lexical/selection": "^0.11.1",
    "@lexical/utils": "^0.11.1",
    "@mdi/js": "^7.2.96",
    "@mdi/react": "^1.6.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/ui": "^0.31.0",
    "jsdom": "^22.0.0",
    "mdast": "^3.0.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-dts": "^1.0.4",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.31.0"
  }
}
